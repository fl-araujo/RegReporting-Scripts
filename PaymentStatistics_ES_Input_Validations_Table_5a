    '02 - BDE VALIDATIONS - 02.05. Check that epigraph 4121 = 41216 + 41217' AS validation_check,
    (CASE WHEN COALESCE(SUM(CASE WHEN "Epigrafe" = 4121 THEN "Numero de operaciones/Volumen" END), 0) = COALESCE(SUM(CASE WHEN "Epigrafe" = 41216 THEN "Numero de operaciones/Volumen" END), 0) + COALESCE(SUM(CASE WHEN "Epigrafe" = 41217 THEN "Numero de operaciones/Volumen" END), 0) AND COALESCE(SUM(CASE WHEN "Epigrafe" = 4121 THEN "Importe" END), 0) = COALESCE(SUM(CASE WHEN "Epigrafe" = 41216 THEN "Importe" END), 0) + COALESCE(SUM(CASE WHEN "Epigrafe" = 41217 THEN "Importe" END), 0) THEN 'true' ELSE 'false' END)::TEXT AS result
